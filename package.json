{
	"name": "snow-ai",
	"version": "0.6.41",
	"description": "Agentic coding in your terminal",
	"license": "MIT",
	"bin": {
		"snow": "bundle/cli.mjs"
	},
	"type": "module",
	"keywords": [
		"cli",
		"ai",
		"assistant",
		"bot",
		"terminal"
	],
	"author": "Mufasa",
	"repository": {
		"type": "git",
		"url": "https://github.com/MayDay-wpf/snow-cli.git"
	},
	"bugs": {
		"url": "https://github.com/MayDay-wpf/snow-cli/issues"
	},
	"homepage": "https://github.com/MayDay-wpf/snow-cli#readme",
	"engines": {
		"node": ">=16",
		"npm": ">=8.3.0"
	},
	"scripts": {
		"build": "node scripts/clean-build.cjs && tsc && node build.mjs",
		"build:ts": "tsc",
		"build:bundle": "node build.mjs",
		"dev": "tsc --watch",
		"start": "node bundle/cli.mjs",
		"start:dev": "node build.mjs && node bundle/cli.mjs",
		"link": "npm run build && npm link",
		"unlink": "npm unlink -g snow-ai",
		"prepublishOnly": "npm run build",
		"postinstall": "node scripts/postinstall.cjs",
		"test": "prettier --check . && xo && ava",
		"lint": "xo",
		"format": "prettier --write ."
	},
	"files": [
		"bundle",
		"scripts"
	],
	"dependencies": {
		"ssh2": "^1.17.0"
	},
	"devDependencies": {
		"@inkjs/ui": "^2.0.0",
		"chardet": "^2.1.1",
		"iconv-lite": "^0.7.2",
		"@modelcontextprotocol/sdk": "^1.17.3",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/diff": "^7.0.2",
		"@types/fs-extra": "^11.0.4",
		"@types/markdown-it": "^14.1.2",
		"@types/marked-terminal": "^6.1.1",
		"@types/pdf-parse": "^1.1.5",
		"@types/prettier": "^2.7.3",
		"@types/react": "^18.0.32",
		"@types/sharp": "^0.32.0",
		"@types/sql.js": "^1.4.9",
		"@types/ssh2": "^1.15.5",
		"@types/ws": "^8.5.8",
		"@vdemedes/prettier-config": "^2.0.1",
		"@vercel/ncc": "^0.38.4",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"chokidar": "^4.0.3",
		"cli-highlight": "^2.1.11",
		"diff": "^8.0.2",
		"esbuild": "^0.27.0",
		"esbuild-plugin-copy": "^2.1.1",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"fzf": "^0.5.2",
		"gray-matter": "^4.0.3",
		"http-proxy-agent": "^7.0.2",
		"https-proxy-agent": "^7.0.6",
		"ignore": "^7.0.5",
		"ink": "^5.2.1",
		"ink-gradient": "^3.0.0",
		"ink-select-input": "^6.2.0",
		"ink-spinner": "^5.0.0",
		"ink-testing-library": "^3.0.0",
		"ink-text-input": "^6.0.0",
		"katex": "^0.16.27",
		"mammoth": "^1.11.0",
		"markdown-it": "^14.1.0",
		"markdown-it-math": "^5.2.1",
		"markdown-it-terminal": "^0.4.0",
		"marked": "^15.0.6",
		"marked-terminal": "^7.3.0",
		"meow": "^11.0.0",
		"pdf-parse": "^2.4.5",
		"pptx-parser": "^1.1.7-beta.9",
		"prettier": "^2.8.7",
		"puppeteer-core": "^24.25.0",
		"react": "^18.2.0",
		"sql.js": "^1.13.0",
		"string-width": "^7.2.0",
		"tiktoken": "^1.0.22",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"undici": "^7.16.0",
		"vscode-jsonrpc": "8.2.1",
		"vscode-languageserver-protocol": "^3.17.5",
		"ws": "^8.14.2",
		"xlsx": "^0.18.5",
		"xo": "^0.53.1"
	},
	"overrides": {
		"glob": "^10.0.0",
		"rimraf": "^5.0.0"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off",
			"react-hooks/rules-of-hooks": "error",
			"react-hooks/exhaustive-deps": "warn"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"optionalDependencies": {
		"sharp": "^0.34.5"
	}
}
